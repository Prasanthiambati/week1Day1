package week1.day1;

public class Fibbinociseries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x=0;
		int y=1;
	int sum=0;
	
	System.out.println(x);
	System.out.println(y);
	
	for (int i=0;i<10;i++)
		{
		 sum=x+y;
		x=y;
		 y=sum;
		 System.out.println(sum);
		}
		 

	}

}
